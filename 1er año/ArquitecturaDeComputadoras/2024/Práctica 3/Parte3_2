EOI EQU 20H
IMR EQU 21H
PB EQU 31H
CB EQU 33H
INT0 EQU 24H
IDINT0 EQU 26

ORG 104
DW SUBRUT_F10

ORG 2000H
CLI
  MOV AL, 0
  OUT CB, AL
  OUT PB, AL
  MOV AL, 11111110B
  OUT IMR, AL
  MOV AL, IDINT0
  OUT INT0, AL
STI

MOV AH, 0
MAIN: JMP MAIN

ORG 3000H
SUBRUT_F10:
  XOR AH, 0FFH
  MOV AL, AH
  OUT PB, AL
  MOV AL, EOI
  OUT EOI, AL
IRET
END
